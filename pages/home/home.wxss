#home {
  padding-bottom: 220rpx;
}

.swiper {
  width: 750rpx;
  height: 652rpx;
}

.swiper .banner-image {
  position: relative;
}

.banner-title {
  width: 278rpx;
  position: absolute;
  left: 55rpx;
  top: 63rpx;
  z-index: 1;
}

.banner-image {
  width: 100%;
  height: 562rpx;
}

/* dot定位位置 */

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 40rpx;
  display: flex;
  justify-content: center;
}

/* 未经过前dot */

.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background-color: transparent;
  border-radius: 8rpx;
  border: 0.1rpx solid #31c27c;
  transition: all 0.6s ease-in-out;
}

/* 经过后dot */

.swiper-container .dots .dot.active {
  width: 24rpx;
  background: #31c27c;
}

/* 二维码 s */

.qr-code {
  text-align: center;
}

.qr-code .qr-code-title image {
  width: 317rpx;
}

.qr-code .camera image {
  width: 146rpx;
}

.qr-code .camera {
  margin: 25rpx 0 5rpx 0;
}

.qr-code .camera-txt {
  font-size: 18rpx;
  color: #808181;
}

.qr-code .form-or {
  margin: 20rpx 0;
}

.qr-code .form-or > text {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  background-image: linear-gradient(to right, #75e1b6, #1ea6d6);
  font-size: 24rpx;
  color: #fff;
  border-radius: 18rpx;
}

/* 二维码 e */

/* 表单s */

/* #1ea6d6  #75e1b6 */

.form {
  width: 750rpx;
  padding: 0 54rpx 34rpx 54rpx;
}

.form .form-view .common-view {
  display: flex;
  height: 53rpx;
  line-height: 53rpx;
}

.form .form-view .left-c {
  position: relative;
  flex: 1;
  padding-left: 67rpx;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
  box-sizing: border-box;
  /* height: 49rpx; *//* line-height: 49rpx; */
  background-color: #fff;
  color: #969697;
  font-size: 0;
}

.form .form-view .plan-left-c {
  position: relative;
  flex: 1;
  padding-left: 67rpx;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
  box-sizing: border-box;
  height: 53rpx;
  line-height: 53rpx;
  background-color: #fff;
  color: #969697;
  font-size: 20rpx;
  border: 2rpx solid #969697;
}

.form .form-view .plan-left-c .icon_down_arrow {
  position: absolute;
  right: 10rpx;
  top: 0;
}

.form .form-view .plan-left-c image {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 12rpx;
  left: 26rpx;
  color: #fff;
  vertical-align: middle;
}

.form .form-view .plan-left-c .text {
  height: 49rpx;
  line-height: 49rpx;
}

.form .form-view .plan-right-c {
  width: 149rpx;
  background: #969697;
  color: #fff;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
  text-align: center;
  font-size: 25rpx;
  height: 53rpx;
  line-height: 53rpx;
}

.form .form-view .left-c::after {
  position: absolute;
  top: 0;
  bottom: 0;
  left: -2rpx;
  right: -2rpx;
  background: linear-gradient(to left, #75e1b6, #1ea6d6);
  content: '';
  z-index: 1;
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
}

.form .form-view .left-c .left-c-icon {
  position: absolute;
  left: 0;
  top: 2rpx;
  width: 70rpx;
  height: 49rpx;
  border-top-left-radius: 7px;
  border-bottom-left-radius: 7px;
  background-color: #fff;
  z-index: 2;
}

.form .form-view .left-c .left-c-icon image {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  top: 10rpx;
  left: 26rpx;
  color: #fff;
}

.form .form-view .left-c input {
  height: 49rpx;
  font-size: 20rpx;
  position: absolute;
  width: calc(100% - 70rpx);
  left: 70rpx;
  top: 2rpx;
  z-index: 2;
  background-color: #fff;
}

.form .form-view .right-c {
  width: 149rpx;
  background: linear-gradient(to left, #1ea6d6, #75e1b6);
  color: #fff;
  border-top-right-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
  text-align: center;
  font-size: 25rpx;
  height: 53rpx;
  line-height: 53rpx;
  box-sizing: border-box;
}

/* 表单e */

/* 导航列表 s */

.navigation-list {
  width: 750rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: linear-gradient(to bottom, #f8fbf8, #fff, #f8fbf8);
  padding: 0 50rpx;
}

.navigation-list > view.navigation-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.navigation-list > view.navigation-item>button {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  z-index: 1;
}

.navigation-list > view.navigation-item > view.navigation-image {
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}

.navigation-list > view.navigation-item text {
  font-size: 24rpx;
  color: #727171;
}

