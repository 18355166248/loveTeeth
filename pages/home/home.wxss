.mind-title {
  text-align: center;
  margin: 40rpx 0 28rpx 0;
}

.mind-title image {
  width: 194rpx;
}

/* 卡列表 s */

.card-box {
  display: flex;
  flex-wrap: wrap;
  align-content: center;
  justify-content: space-between;
  padding: 0 45rpx;
}

.card-box .card-item {
  width: 316rpx;
  height: 250rpx;
  padding: 7rpx 7rpx 0 7rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-shadow: 1rpx 1rpx 5rpx 0 rgba(0, 0, 0, 0.2);
  margin-bottom: 27rpx;
  border-radius: 12rpx;
}

.card-box .card-item image {
  width: 100%;
}

.card-box .card-item .card-bg {
  font-size: 24rpx;
  color: #727171;
  margin-top: 10rpx;
}

